LPARAMETERS VlN_accion,VlN_entidad,Vlc_descri,Vlc_motivo,vln_idmod

Vln_accion=Vln_accion
Vln_entidad=Vln_entidad
Vlc_descri=ALLTRIM(UPPER(Vlc_descri))
Vlc_motivo=ALLTRIM(UPPER(Vlc_motivo))

IF conex > 0 
	
		lsql="INSERT INTO ETB_bitacora (cedula,id_accion,id_entidad,descripcion,motivo,id_mod)"+;
		" values(?Vgc_usuario,?Vln_accion,?Vln_entidad,?Vlc_descri,?Vlc_motivo,?vln_idmod)"
		resp=SQLEXEC(conex, lsql, "MiCursor")
		IF resp<=0
			MESSAGEBOX("Disculpe, error en la consulta por favor comunicarse con el Dpto. de Informática .",0+16,"Error de conexión")
		ENDIF
	
ELSE
	MESSAGEBOX("Disculpe, no existe conexión con el servidor.",0+16,"Error de conexión")
ENDIF 